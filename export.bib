   pages = {440-445},
   publisher = {IEEE},
   title = {Role of sodium in the control of defect structures in CIS [solar cells]},
   volume = {2000-Janua},
   url = {http://ieeexplore.ieee.org/document/915864/},
   year = {2000}
}
@inproceedings{Ahrenkiel2000a,
   abstract = {We examine CuPt-B atomic sublattice ordering in Gao. siIno. 49P (GalnP) and Gao. 47lno. 53As (GalnAs) III-V alloy films grown by atmospheric- and low-pressure metalorganic chemical vapor deposition on singular and vicinal (001) substrates. The influences of growth conditions and substrate miscut on double- and single-variant ordered microstructures are investigated using transmission electron microscopy (TEM). Relatively thick ( > 1-2 urn) double-variant ordered GalnP and GalnAs films show complementary superdomain formation. Single-variant ordered films on < lll > B-miscut substrates contain single-phase domains, separated by antiphase boundaries (APBs). The appearance of APBs in TEM dark-field images is anticipated from electron diffraction theory. © 2000 Materials Research Society.},
   author = {S.P. Ahrenkiel and K.M. Jones and R.J. Maison and M.M. Al-Jassim and Y. Zhang and A. Mascarenhas and D.J. Friedman and D.J. Arent and J.M. Olson and M.C. Hanna},
