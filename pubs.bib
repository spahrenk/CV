   month = {7},
   pages = {2338-2344},
   title = {Identification of Nucleation Center Sites in Thermally Annealed Hydrogenated Amorphous Silicon},
   volume = {19},
   url = {https://advanced.onlinelibrary.wiley.com/doi/10.1002/adfm.200801709},
   year = {2009}
